    "db:studio": "drizzle-kit studio --port 3131 --verbose",
    "db:clear": "bun ./src/db/clear.ts",
    "db:seed": "bun ./src/db/seed.ts",
    "db:reset": "bun run db:clear && bun run db:migrate && bun run db:seed"
  },
